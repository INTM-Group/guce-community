{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "baseUrl": "./",
    "paths": {
      "@storage/*": [
        "./storage/public/*"
      ],
      "@public/*": [
        "./public/*"
      ],
      "@@/*": [
        "./storage/public/*"
      ],
      "@/*": [
        "./src/*"
      ],
      "~~/*": [
        "./src/*"
      ],
      "~/*": [
        "./*"
      ]
    }
  },
  "include": [
    "src/**/*.vue",
    "src/**/*.js"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
